//Addition of the number of students and teachers in a class
//created by Holly Chen on 26/02/2021
#include <stdio.h>
int main(void){
    int a;
    int b;
    printf("Please enter the number of students and tutors: \n");
    scanf("%d %d", &a,&b);
    printf("%d + %d = %d\n", a, b, a+b);
    return 0;
}
